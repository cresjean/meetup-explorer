{
  "template": "rsvps*",
  "settings": {
    "number_of_shards": 1
  },
  "mappings": {
    "rsvp": {
      "_source": {
        "enabled": false
      },
      "properties": {
        "venue": {
          "properties": {
            "venue_name": {
              "type": "string"
            },
            "location": {
                "type": "geo_point"
            },

            "venue_id": {
              "type": "string",
              "index": "not_analyzed"
            }
          }
        },
        "mtime": {
          "type": "date",
          "index": "not_analyzed"
        },
        "event": {
          "properties": {
            "event_name": {
              "type": "string"
            },
            "time": {
              "type": "date"
            }
          }
        },
        "group": {
          "properties": {
            "group_topics": {
              "properties": {
                "urlkey": {
                  "type": "string"
                },
                "topic_name": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
